@gray-base: #000;
@gray-darker: #3e3f3a;
@gray-dark: #8e8c84;
@gray: #98978b;
@gray-light: #dfd7ca;
@gray-lighter: #f8f5f0;

@primaryColor: #556475;
@positiveColor: #4ba09c;
@positiveTextColor: #4ba09c;
@infoColor: #29abe0;
@infoTextColor: #29abe0;
@warningColor: #f47c3c;
@warningTextColor: #f47c3c;
@negativeColor: #d9534f;
@negativeTextColor: #d9534f;

@textColor: #3e3f3a;
@fontName: "Roboto";
@emSize: 12.25px;
@linkColor: @positiveColor;
